# Brevo Email Configuration
# Get your SMTP password from your Brevo account dashboard
BREVO_PASSWORD=your_actual_brevo_smtp_password_here

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google reCAPTCHA Enterprise Configuration
# Get your reCAPTCHA Enterprise keys from https://cloud.google.com/recaptcha-enterprise
# Site Key (public - safe to expose) - This is your actual key
VITE_RECAPTCHA_SITE_KEY=your_recaptcha_enterprise_site_key_here
# Secret Key (private - keep secure) - Get from Google Cloud Console
VITE_RECAPTCHA_SECRET_KEY=your_recaptcha_enterprise_secret_key_here
# reCAPTCHA Type - Enterprise uses v3 invisible scoring
VITE_RECAPTCHA_VERSION=enterprise